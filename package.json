{
  "name": "amo-blocklist-ui",
  "version": "0.0.1",
  "description": "AMO blocklist UI.",
  "scripts": {
    "build": "NODE_ENV=production webpack --optimize-minimize --config webpack.prod.js && cp index.html build/",
    "lint": "eslint scripts test",
    "publish": "npm run build && gh-pages --dist build/",
    "start": "node devServer.js",
    "tdd": "npm test -- -w",
    "test": "NODE_ENV=test mocha --compilers js:babel/register --recursive --require ./test/setup-jsdom.js $(find test -name '*_test.js')"
  },
  "dependencies": {
    "btoa": "^1.1.2",
    "extract-text-webpack-plugin": "^0.9.1",
    "jsonschema": "^1.0.2",
    "kinto": "^1.1.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.4",
    "redux-simple-router": "0.0.10",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.20",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "chai": "^3.3.0",
    "css-loader": "^0.15.6",
    "eslint": "^1.8.0",
    "eslint-plugin-react": "^3.6.3",
    "express": "^4.13.3",
    "gh-pages": "^0.4.0",
    "jsdom": "^7.1.0",
    "mocha": "^2.3.0",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "sinon": "^1.17.2",
    "style-loader": "^0.12.3",
    "webpack": "^1.10.5",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mozilla-services/amo-blocklist-ui.git"
  },
